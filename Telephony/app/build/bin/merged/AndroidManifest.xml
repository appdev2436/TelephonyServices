<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.telephony2020"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:resizeableActivity="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.telephony2020.splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.telephony2020.MainActivity" >
        </activity>
        <activity android:name="com.telephony2020.SecondActivity" >
        </activity>
        <activity android:name="com.telephony2020.ThirdActivity" >
        </activity>
        <activity android:name="com.telephony2020.FourthActivity" >
        </activity>
        <activity android:name="com.telephony2020.FifthActivity" >
        </activity>
        <activity android:name="com.telephony2020.SixActivity" >
        </activity>
    </application>

</manifest>